<%# 記憶節約率を表現したグラフ %>
<%= area_chart memory_point_array(post),
          id: 'total-attendance-chart',
          width: '100%',
          height: '400px',
          marigin_top: '30px',
          colors: ['#F99F48'],
          xtitle: "時間経過", ytitle: "節約率",
          xmin:  post.created_at.yesterday.strftime('%m/%d'),
          xmax: post.created_at.since(10.days).strftime('%m/%d'),
          min: 0, max: 100,
          points: false, curve: false,
          messages: {empty: "グラフデータがありません"},
          discrete: true
          %>